<a class="thumbnailContainer" href="/movie/{{movieId}}">
    
    <div class="thumbnailImageContainer">
        <img src={{posterUrl}} height="150">
    </div>
    <div class="thumbnailTitleContainer">
        
        <h2>{{title}} ({{year}})</h2>
    </div>
    <div class="thumbnailRating">
        <h2>{{averageRating}}/5</h2>
    </div>
</a>